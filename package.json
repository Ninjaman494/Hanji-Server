    "apollo-server": "3.10.0",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "3.10.0",
    "async": "3.2.4",
    "dotenv": "14.3.2",
    "express": "4.18.1",
    "firebase-admin": "10.3.0",
    "glob": "7.2.3",
    "graphql": "16.5.0",
    "graphql-rate-limit-directive": "^1.3.0",
    "graphql-upload": "^13.0.0",
    "lodash": "^4.17.21",
    "mongodb": "4.8.1",
    "node-cron": "3.0.1",
    "path": "0.12.7",
    "request": "2.88.2",
    "request-promise": "4.2.6"
