{
  "name": "hanji-server",
  "version": "2.3.0",
  "description": "JavaScript Server for conjugating Korean verbs/adjectives. Uses Dan Bravender's source code for conjugation.",
  "type": "commonjs",
  "author": "Akash Eldo",
  "license": "Apache-2.0",
  "scripts": {
    "start": "cross-env TS_NODE_BASEURL=./build node -r tsconfig-paths/register build/main.js",
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts,",
    "test": "jest",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "gcp-build": "npm run build",
    "gen-graphql": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ninjaman494/KoreanConjugator.git"
  },
  "dependencies": {
    "@google-cloud/debug-agent": "7.1.0",
    "@slack/web-api": "6.7.2",
    "apollo-datasource": "^3.3.0",
    "apollo-server": "3.11.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-express": "3.11.1",
    "async": "3.2.4",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "firebase-admin": "11.2.0",
    "glob": "8.0.3",
    "graphql": "^15.8.0",
    "graphql-rate-limit-directive": "^1.3.0",
    "graphql-upload": "^13.0.0",
    "lodash": "^4.17.21",
    "mongodb": "4.11.0",
    "node-cron": "3.0.2",
    "path": "0.12.7",
    "request": "2.88.2",
    "request-promise": "4.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@graphql-codegen/cli": "2.13.11",
    "@graphql-codegen/typescript": "2.8.1",
    "@graphql-codegen/typescript-resolvers": "2.7.6",
    "@shelf/jest-mongodb": "^4.1.3",
    "@types/express": "4.17.14",
    "@types/graphql-upload": "^8.0.6",
    "@types/jest": "^29.1.1",
    "@types/lodash": "^4.14.184",
    "@types/mongodb": "4.0.6",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "apollo-server-testing": "2.25.3",
    "babel-jest": "^29.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "casual": "1.6.2",
    "cross-env": "^7.0.3",
    "eslint": "8.27.0",
    "jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.4"
  }
