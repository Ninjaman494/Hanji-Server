    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@shelf/jest-mongodb": "^2.1.1",
    "@types/express": "4.17.13",
    "@types/graphql-upload": "^8.0.6",
    "@types/jest": "27.4.1",
    "@types/mongodb": "4.0.6",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
